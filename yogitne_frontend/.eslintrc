{
  "extends": "airbnb",
  "rules": {
      "class-methods-use-this": 0,
      "comma-dangle": 0,
      "react/prop-types": 0,
      "linebreak-style": 0,
      "no-console": 'off',
      "no-lonely-if": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/prefer-stateless-function": [0, { "ignorePureComponents": <ignorePureComponents> }],
      "jsx-a11y/label-has-for": [ 2, {
            "components": [ "Label" ],
            "required": {
                "some": [ "nesting", "id" ]
            },
            "allowChildren": true,
      }],
      "no-param-reassign": 0,
    },
  "env": {
      "browser": true,
      "node": true
  }
}
